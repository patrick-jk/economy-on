<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.economiaon.ui.navigation.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_overview_finances"
        android:name="com.economiaon.ui.navigation.finances.FinancesOverviewFragment"
        android:label="@string/title_finances"
        tools:layout="@layout/fragment_overview_finances" />

    <fragment
        android:id="@+id/navigation_finance_history"
        android:name="com.economiaon.ui.navigation.financehistory.FinanceHistoryFragment"
        android:label="@string/title_latest_finances"
        tools:layout="@layout/fragment_latest_finances" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.economiaon.ui.navigation.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>